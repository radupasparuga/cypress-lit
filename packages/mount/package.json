{
  "name": "@cypress-lit/mount",
  "description": "A cypress with web components that works for lit elements and custom elements registered with pure JS",
  "version": "0.0.1",
  "author": "Simon Reilly",
  "devDependencies": {
    "@cypress/mount-utils": ">=2.1.0",
    "cypress": "^10.10.0",
    "lit": "^2.3.1",
    "prettier": "^2.7.1",
    "rollup": "^3.2.5",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "release-it": "^15.5.0"
  },
  "files": [
    "dist/**/*",
    "types/**/*"
  ],
  "homepage": "https://github.com/simonireilly/cypress-lit",
  "keywords": [
    "cypress",
    "lit",
    "testing",
    "web components"
  ],
  "license": "MIT",
  "main": "./dist/mount.js",
  "module": "./dist/mount.mjs",
  "peerDependencies": {
    "@cypress/mount-utils": ">=2.1.0",
    "cypress": ">=10.6.0",
    "svelte": ">=3.0.0"
  },
  "repository": "simonireilly/cypress-lit",
  "scripts": {
    "build": "vite build",
    "compile": "tsc --build",
    "cypress": "cypress",
    "lint": "prettier './{src,cypress}/**/*.{js,ts}' --write",
    "pub:test": "release-it --preRelease=alpha --npm.tag=alpha --dry-run",
    "pub:canary": "release-it --preRelease=alpha --npm.tag=alpha",
    "pub:main": "release-it patch"
  },
  "types": "types/index.d.ts"
}