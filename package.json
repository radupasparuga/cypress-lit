{
  "name": "cypress-lit",
  "description": "Workspace for cypress-lit utils",
  "version": "1.0.0",
  "author": "Simon Reilly",
  "main": "dist/cypress-lit.js",
  "exports": {
    ".": "./dist/cypress-lit.js"
  },
  "types": "types/cypress-lit.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "devDependencies": {
    "@cypress/mount-utils": ">=3.0.0",
    "@types/node": "^18.11.9",
    "cypress": "^11.0.0",
    "embedme": "^1.22.1",
    "husky": "^8.0.2",
    "lit": "^2.4.1",
    "prettier": "^2.7.1",
    "rollup": "^3.2.5",
    "syncpack": "^8.3.9",
    "typescript": "^4.8.4",
    "vite": "^3.2.3"
  },
  "keywords": [
    "cypress",
    "lit",
    "testing",
    "web components"
  ],
  "license": "MIT",
  "peerDependencies": {
    "@cypress/mount-utils": ">=2.1.0",
    "cypress": ">=10.6.0"
  },
  "private": "true",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc && vite build",
    "docs": "embedme ./README.md",
    "fmt": "syncpack format",
    "lint": "prettier './{src,cypress,templates}/**/*.{js,ts}' --write",
    "prepare": "husky install"
  }
}